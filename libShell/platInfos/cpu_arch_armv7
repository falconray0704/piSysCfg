
# os arch (get by uname -m)
export OSENV_OS_CPU_ARCH="armv7l"


# docker cpu arch, 
# refer to: https://github.com/docker-library/official-images#multiple-architectures
export OSENV_DOCKER_CPU_ARCH="arm32v7"


# docker tag cpu arch, 
# refer to the tags in: https://hub.docker.com/_/ubuntu?tab=tags&page=1&ordering=last_updated
export OSENV_DOCKER_TAG_CPU_ARCH="arm/v7"

